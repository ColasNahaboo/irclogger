# This file is not needed anymore actually. 
# just here for reference, can be useful one day...


quote_url () {
s="${1//[%]/%25}"
s="${s///%01}"
s="${s///%02}"
s="${s///%03}"
s="${s///%04}"
s="${s///%05}"
s="${s///%06}"
s="${s///%07}"
s="${s///%08}"
s="${s//	/%09}"
s="${s//
/%0a}"
s="${s///%0b}"
s="${s///%0c}"
s="${s///%0d}"
s="${s///%0e}"
s="${s///%0f}"
s="${s///%10}"
s="${s///%11}"
s="${s///%12}"
s="${s///%13}"
s="${s///%14}"
s="${s///%15}"
s="${s///%16}"
s="${s///%17}"
s="${s///%18}"
s="${s///%19}"
s="${s///%1a}"
s="${s///%1b}"
s="${s///%1c}"
s="${s///%1d}"
s="${s///%1e}"
s="${s///%1f}"
s="${s// /%20}"
s="${s//!/%21}"
s="${s//\"/%22}"
s="${s//[#]/%23}"
s="${s//$/%24}"
s="${s//&/%26}"
s="${s//\'/%27}"
s="${s//(/%28}"
s="${s//)/%29}"
s="${s//,/%2c}"
s="${s//:/%3a}"
s="${s//;/%3b}"
s="${s//</%3c}"
s="${s//=/%3d}"
s="${s//>/%3e}"
s="${s//[?]/%3f}"
s="${s//@/%40}"
s="${s//[[]/%5b}"
s="${s//\\/%5c}"
s="${s//[]]/%5d}"
s="${s//^/%5e}"
s="${s//\`/%60}"
s="${s//{/%7b}"
s="${s//|/%7c}"
s="${s//\}/%7d}"
s="${s//~/%7e}"
s="${s///%7f}"
s="${s//Ä/%80}"
s="${s//Å/%81}"
s="${s//Ç/%82}"
s="${s//É/%83}"
s="${s//Ñ/%84}"
s="${s//Ö/%85}"
s="${s//Ü/%86}"
s="${s//á/%87}"
s="${s//à/%88}"
s="${s//â/%89}"
s="${s//ä/%8a}"
s="${s//ã/%8b}"
s="${s//å/%8c}"
s="${s//ç/%8d}"
s="${s//é/%8e}"
s="${s//è/%8f}"
s="${s//ê/%90}"
s="${s//ë/%91}"
s="${s//í/%92}"
s="${s//ì/%93}"
s="${s//î/%94}"
s="${s//ï/%95}"
s="${s//ñ/%96}"
s="${s//ó/%97}"
s="${s//ò/%98}"
s="${s//ô/%99}"
s="${s//ö/%9a}"
s="${s//õ/%9b}"
s="${s//ú/%9c}"
s="${s//ù/%9d}"
s="${s//û/%9e}"
s="${s//ü/%9f}"
s="${s//†/%a0}"
s="${s//°/%a1}"
s="${s//¢/%a2}"
s="${s//£/%a3}"
s="${s//§/%a4}"
s="${s//•/%a5}"
s="${s//¶/%a6}"
s="${s//ß/%a7}"
s="${s//®/%a8}"
s="${s//©/%a9}"
s="${s//™/%aa}"
s="${s//´/%ab}"
s="${s//¨/%ac}"
s="${s//≠/%ad}"
s="${s//Æ/%ae}"
s="${s//Ø/%af}"
s="${s//∞/%b0}"
s="${s//±/%b1}"
s="${s//≤/%b2}"
s="${s//≥/%b3}"
s="${s//¥/%b4}"
s="${s//µ/%b5}"
s="${s//∂/%b6}"
s="${s//∑/%b7}"
s="${s//∏/%b8}"
s="${s//π/%b9}"
s="${s//∫/%ba}"
s="${s//ª/%bb}"
s="${s//º/%bc}"
s="${s//Ω/%bd}"
s="${s//æ/%be}"
s="${s//ø/%bf}"
s="${s//¿/%c0}"
s="${s//¡/%c1}"
s="${s//¬/%c2}"
s="${s//√/%c3}"
s="${s//ƒ/%c4}"
s="${s//≈/%c5}"
s="${s//∆/%c6}"
s="${s//«/%c7}"
s="${s//»/%c8}"
s="${s//…/%c9}"
s="${s// /%ca}"
s="${s//À/%cb}"
s="${s//Ã/%cc}"
s="${s//Õ/%cd}"
s="${s//Œ/%ce}"
s="${s//œ/%cf}"
s="${s//–/%d0}"
s="${s//—/%d1}"
s="${s//“/%d2}"
s="${s//”/%d3}"
s="${s//‘/%d4}"
s="${s//’/%d5}"
s="${s//÷/%d6}"
s="${s//◊/%d7}"
s="${s//ÿ/%d8}"
s="${s//Ÿ/%d9}"
s="${s//⁄/%da}"
s="${s//€/%db}"
s="${s//‹/%dc}"
s="${s//›/%dd}"
s="${s//ﬁ/%de}"
s="${s//ﬂ/%df}"
s="${s//‡/%e0}"
s="${s//·/%e1}"
s="${s//‚/%e2}"
s="${s//„/%e3}"
s="${s//‰/%e4}"
s="${s//Â/%e5}"
s="${s//Ê/%e6}"
s="${s//Á/%e7}"
s="${s//Ë/%e8}"
s="${s//È/%e9}"
s="${s//Í/%ea}"
s="${s//Î/%eb}"
s="${s//Ï/%ec}"
s="${s//Ì/%ed}"
s="${s//Ó/%ee}"
s="${s//Ô/%ef}"
s="${s///%f0}"
s="${s//Ò/%f1}"
s="${s//Ú/%f2}"
s="${s//Û/%f3}"
s="${s//Ù/%f4}"
s="${s//ı/%f5}"
s="${s//ˆ/%f6}"
s="${s//˜/%f7}"
s="${s//¯/%f8}"
s="${s//˘/%f9}"
s="${s//˙/%fa}"
s="${s//˚/%fb}"
s="${s//¸/%fc}"
s="${s//˝/%fd}"
s="${s//˛/%fe}"
s="${s//ˇ/%ff}"
echo "$s"
}

